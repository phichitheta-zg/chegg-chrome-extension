{
  "name": "Chegg",
  "version": "1.0",
  "description": "An Extension for Chegg",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://www.chegg.com/"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "PCT-logo.png"
  }
}
